datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  brand       String?
  brandCode   String?
  cart        Boolean?
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  model       String?
  oem         String?
  price       Float?
  productName String?
  updatedAt   DateTime @updatedAt
}

model User {
  createdAt DateTime      @default(now())
  email     String?       @unique
  firstName String?
  id        String        @id @default(cuid())
  lastName  String?
  password  String
  role      EnumUserRole?
  roles     Json
  updatedAt DateTime      @updatedAt
  username  String        @unique
}

enum EnumUserRole {
  Option1
}
